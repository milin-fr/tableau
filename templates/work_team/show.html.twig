{% extends 'base.html.twig' %}

{% block title %}WorkTeam{% endblock %}

{% block body %}
    <h1>WorkTeam</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ work_team.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ work_team.title }}</td>
            </tr>
            <tr>
                <th>Enabled</th>
                <td>{{ work_team.enabled ? 'Yes' : 'No' }}</td>
            </tr>
